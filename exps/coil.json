{
    "prefix": "reproduce",
    "dataset": "cifar100",
    "memory_size": 2000,
    "memory_per_class": 2,
    "fixed_memory": true,
    "shuffle": true,
    "init_cls": 10,
    "increment": 10,
    "sinkhorn":0.464,
    "calibration_term":1.5,
    "norm_term":3.0,
    "reg_term":1e-3,
    "model_name": "coil",
    "convnet_type": "cosine_resnet32",
    "device": ["0"],
    "seed": [1993],
    "selection_epochs": 2,
    "uncertainty": "Entropy",
    "balance": true,
    "submodular_greedy": "LazyGreedy",
    "submodular": "GraphCut",
    "selection": "submodular",
    "fraction": 0.5,
    "selection_lr": 0.1,
    "selection_momentum": 0.9,
    "selection_weight_decay":5e-4,
    "selection_nesterov": true,
    "selection_batch": 128,
    "workers": 8,
    "selection_optimizer": "SGD",
    "print_freq":20
}
